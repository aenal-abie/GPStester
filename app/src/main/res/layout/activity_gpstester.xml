<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".GPStester">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:text="GPS Fix"
                android:id="@+id/GPSFIX"
                android:layout_gravity="start"/>
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:text="-"
                android:id="@+id/midt"
                android:layout_gravity="start"/>
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:text="-"
                android:id="@+id/rightt"
                android:layout_gravity="start"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:text="-"
                android:id="@+id/leftb"
                android:layout_gravity="start"/>
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:text="-"
                android:id="@+id/midb"
                android:layout_gravity="start"/>
            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:text="count"
                android:id="@+id/count"
                android:layout_gravity="start"/>
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:text="LIST ALL"
            android:id="@+id/MessageTypes"
            android:layout_gravity="start"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:text="Device Data"
            android:id="@+id/DevInfo"
            android:layout_gravity="start"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:text="GGA"
            android:id="@+id/GGA"
            android:layout_gravity="start"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:text="GSA"
            android:id="@+id/GSA"
            android:layout_gravity="start"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:text="GSV"
            android:id="@+id/GSV"
            android:layout_gravity="start"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:text="RMC"
            android:id="@+id/RMC"
            android:layout_gravity="start"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:text="VTG"
            android:id="@+id/VTG"
            android:layout_gravity="start"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:text="Glonass"
            android:id="@+id/GL"
            android:layout_gravity="start"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:text="Beidou"
            android:id="@+id/BD"
            android:layout_gravity="start"/>

    </LinearLayout>
</RelativeLayout>
